{% extends 'KatropineAdminBundle::layout.html.twig' %}

{% block title %}{{'Add_new'|trans}} {{'company'|trans}}{% endblock %}

{% block bodyheading %}
    {% include 'KatropineAdminBundle:partials:bodyheading.html.twig' with {'title' : 'Add_new'|trans } %}
{% endblock %}

{% block body %}
    {% if company is defined and company is not null %}
    <div>
        <table class="table">
            <tr>
                <td>{{'Id'|trans}}: {{company.id}}</td>
                <td>{{'Created'|trans}}: {{company.created|date('Y-m-d H:i:s')}}</td>
                <td>{{'Updated'|trans}}: {{company.modified|date('Y-m-d H:i:s')}}</td>
            </tr>
        </table>
    </div>
    {% endif %}        
    <div>      
        {{ form(form) }}
    </div>
{% endblock %}    